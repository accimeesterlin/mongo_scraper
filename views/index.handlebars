<div id="wrapper">
       <div class="col-md-12 jumbotron">
        <div class="text-center">
            <h1> Relix Scraper</h1>
        </div>

    </div>

    <div id="articles" class="col-md-12">
         {{!-- {{#if articles}} --}}
        {{#each data}}
        <div class="panel panel-default">
            <div class="panel-body" id = {{this._id}}>
                <h4 class="title"> {{this.title}}</h4>
                <a href="{{this.link}}" target= "_blank"> {{this.link}}</a>
                <p>{{this.summary}}</p>
                <br><br>
                <div>
                    <h2>{{this.note.title}}</h2>
                    <p>{{this.note.body}}</p>
                </div>
                <button type="button" class="btn btn-success save" data-id={{this._id}}>Save Article</button>
                <button type="button" class="btn btn-success note" data-id={{this._id}} >Add Note</button>
            </div>
        </div>


        {{/each}}
    <div id="notes"></div>
   
    </div>

</div>